.custom-tab-bar{align-items:center;background:#fff;border-top:1rpx solid rgba(0,0,0,.1);bottom:0;display:flex;height:100rpx;justify-content:space-around;left:0;padding-bottom:env(safe-area-inset-bottom);position:fixed;right:0;z-index:1000}.tab-item{align-items:center;display:flex;flex:1;height:100%;justify-content:center}.tab-text{color:#666;font-size:40rpx;font-weight:500;transition:color .3s}.tab-item.active .tab-text{color:#8fbc8f;font-weight:600}