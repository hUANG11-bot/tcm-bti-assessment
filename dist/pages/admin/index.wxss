.admin-page{background:linear-gradient(180deg,#fcfcfc,#fff);min-height:100vh}.admin-container{display:flex;flex-direction:column;gap:40rpx;padding:40rpx 30rpx}.page-header{margin-bottom:20rpx;text-align:center}.page-title{color:#2d3748;display:block;font-size:64rpx;font-weight:700;margin-bottom:16rpx}.page-subtitle{color:#718096;display:block;font-size:32rpx}.empty-state,.loading-state{align-items:center;display:flex;flex-direction:column;gap:40rpx;justify-content:center;min-height:60vh}.empty-title,.loading-text{color:#2d3748;font-size:48rpx;font-weight:700}.empty-text{color:#718096;font-size:32rpx}.stats-grid{display:grid;gap:24rpx;grid-template-columns:repeat(3,1fr)}.stat-value{color:#8fbc8f;display:block;font-size:64rpx;font-weight:700}.distribution-list{display:flex;flex-direction:column;gap:24rpx}.distribution-item{align-items:center;display:flex;gap:16rpx}.distribution-name{color:#2d3748;font-size:28rpx;min-width:120rpx}.distribution-bar-wrapper{background:hsla(0,0%,95%,.8);border-radius:16rpx;flex:1;height:32rpx;overflow:hidden}.distribution-bar{background:#8fbc8f;height:100%;transition:width .3s}.distribution-value{color:#2d3748;font-size:28rpx;font-weight:700;min-width:60rpx;text-align:right}.gender-grid{display:grid;gap:32rpx;grid-template-columns:repeat(2,1fr)}.gender-item{align-items:center;background:hsla(0,0%,95%,.5);border-radius:16rpx;display:flex;flex-direction:column;gap:16rpx;padding:32rpx}.gender-name{color:#2d3748;font-size:32rpx;font-weight:500}.gender-value{color:#8fbc8f;font-size:48rpx;font-weight:700}.assessments-list{display:flex;flex-direction:column;gap:16rpx}.assessment-item{background:hsla(0,0%,95%,.5);border-radius:12rpx;padding:24rpx}.assessment-info{display:flex;flex-direction:column;gap:8rpx}.assessment-type{color:#2d3748;font-size:32rpx;font-weight:700}.assessment-meta{color:#718096;font-size:24rpx}